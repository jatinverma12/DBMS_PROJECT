<body>
<div id="main">
  <%-include('AdminNav.ejs')%>
 
   <section class="banner-area" style="background: url('/img/home.png')" >
    <div class="container">
      
      <div class="row justify-content-center align-items-center">
        <div class="col-lg-12 banner-right">
          <h1 class="text-white">
              Edit Personal Details
          </h1>
          
        </div>

      
      </div>

    </div>
  </section>
  <div class="card mx-5 my-5 px-5 py-5" style="min-height: 35vh;">
    <h1>Personal Details</h1>
    <form id="main" class="form-horizontal" action="/admin/students/<%= st._id %>?_method=PUT" method="POST" novalidate>
      <div class="form-group row mb-0">
          <div class="col-sm-8 md-form px-0">
            <label for="email" class="control-label">Email</label><br>
            <input id="email" class="form-control" type="email" name="email" value=<%=st.email%>>
            
          </div>
          <div class="col-sm-4 messages"></div>
      </div>
      <div class="form-group row mb-0">
        <div class="col-sm-8 md-form px-0">
          <label for="Enrolment NO." class="control-label">Enrolment No.</label><br>
          <input id="Enrolment NO." class="form-control" type="text" name="eno" value=<%=st.eno%>>
          
        </div>
        <div class="col-sm-4 messages"></div>
      </div>
      <div class="form-group row mb-0">
        <div class="col-sm-8 md-form px-0">
          <label class="control-label" for="registration">Registration No.</label><br>
          <input id="registration" class="form-control" type="number" name="id" value=<%=st._id%>>
          
        </div>
        <div class="col-sm-4 messages"></div>
      </div>
      <div class="form-group row mb-0">
        <div class="col-sm-8 md-form px-0">
          <label class="control-label" for="username">Student Name</label><br>
          <input id="username" class="form-control" type="text" name="sname" value="<%=st.sname%>">
          
        </div>
        <div class="col-sm-4 messages">
        </div>
      </div>

        <div class="form-group row mb-0">
        <div class="col-sm-8 md-form px-0">
          <label class="control-label" for="username">Father Name</label><br>
          <input id="username" class="form-control" type="text" name="fname" value="<%=st.fname%>">
          
        </div>
        <div class="col-sm-4 messages">
        </div>
      </div>


        <div class="form-group row mb-0">
        <div class="col-sm-8 md-form px-0">
          <label class="control-label" for="username">Branch</label><br>
          <input id="username" class="form-control" type="text" name="branch" value="<%=st.branch%>">
          
        </div>
        <div class="col-sm-4 messages">
        </div>
      </div>

       <div class="form-group row mb-0">
        <div class="col-sm-8 md-form px-0">
          <label class="control-label" for="contact">Contact</label><br>
          <input id="contact" class="form-control" type="text" name="contact" value="<%=st.contact%>">
          
        </div>
        <div class="col-sm-4 messages">
        </div>
      </div>


     <!--<h1>Marks</h1>

     -->
    
      
      <div class="form-group">
        <div class="text-center pt-3">
          <button type="submit" class="btn btn-dark">Submit</button>
        </div>
      </div>
    </form>
  </div>
</div>


<script>
  function openNav() {
  document.getElementById("mySidenav").style.width = "250px";
  document.getElementById("main").style.marginLeft = "250px";
  
}

/* Set the width of the side navigation to 0 and the left margin of the page content to 0, and the background color of body to white */
function closeNav() {
  document.getElementById("mySidenav").style.width = "0";
  document.getElementById("main").style.marginLeft = "0";
  
}
</script>

